{
  "name": "molovo/market",
  "description": "A fully fledged e-commerce admin module for Laravel 5.1+",
  "keywords": [
    "molovo",
    "market"
  ],
  "homepage": "https://github.com/molovo/market",
  "license": "MIT",
  "authors": [ {
    "name": "James Dinsdale",
    "email": "hi@molovo.co",
    "homepage": "https://molovo.co",
    "role": "Developer"
  } ],
  "require": {
    "illuminate/support": "~5.1",
    "php": ">=5.3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "4.*",
    "scrutinizer/ocular": "~1.1"
  },
  "autoload": {
    "psr-4": {
      "Molovo\\Market\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Molovo\\Market\\Test\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit",
    "post-update-cmd": [
      "php artisan vendor:publish --tag=public --force"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}